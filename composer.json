{
    "name": "mhunesi/yii2-formio",
    "description": "Yii2 Formio Extension",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/mhunesi/formio/issues?state=open",
        "source": "https://github.com/mhunesi/formio"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "yii2tech/ar-softdelete": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "mhunesi\\formio\\": "src"
        }
    }
}
